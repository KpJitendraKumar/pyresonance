MODULE main
  VAR
    policy : {fwd2,fwd3,fwd4};
    rate : 1..3;
  ASSIGN
   init(rate) := 1;
   init(policy) := fwd2;
   next(policy) := 
     case
       rate=1 : fwd2;
       rate=2 : fwd3;
       rate=3 : fwd4;
     esac;
  SPEC AG (rate=1 -> AX policy=fwd2)
  SPEC AG (rate=2 -> AX policy=fwd3)
  SPEC AG (rate=3 -> AX policy=fwd4)
  SPEC AG (EF policy=fwd2)
  SPEC AG (policy=fwd2 -> EF policy=fwd3)
  SPEC AG (policy=fwd2 -> EF policy=fwd4)
 
